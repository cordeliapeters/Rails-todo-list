<h1>Login here!</h1>
<h1><%= link_to 'Sign Up', register_path %></h1>

<%if @errors%>
<%=@errors%>
<%end%>

<%= form_tag login_path do %>
  <%= label_tag "Email" %>
  <%= text_field_tag :email %>

  <%= label_tag "Password" %>
  <%= password_field_tag :password %>

  <%= submit_tag "Login" %>
<% end %>




